try:
	var1=	input('please provide x ')
	var2=	input('please provide y ')
	operator= input('please provide operator ')
	if operator=='+'  :
		print(var1 + var 2)
	if operator=='-'  :
		print(var1 - var 2)
	if operator=='*'  :
		print(var1 * var 2)
	if operator=='/'  :
		print(var1 / var 2)
except:
	pass